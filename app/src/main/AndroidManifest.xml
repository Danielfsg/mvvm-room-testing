<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.dw.room_livedata_viewmodel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="pt.dw.room_livedata_viewmodel.listitems.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="pt.dw.room_livedata_viewmodel.additem.AddItemActivity" />
        <activity
            android:name="pt.dw.room_livedata_viewmodel.viewitem.ViewItemActivity"
            android:label="@string/title_activity_view_item"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>